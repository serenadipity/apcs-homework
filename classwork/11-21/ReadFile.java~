import java.io.*;
import java.util.*;

public class ReadFile {
    public static void main(String[] args) {
	try {
	    Scanner sc = new Scanner(new File("words.txt"));
	    while (sc.hasNext()) {
		String s = sc.next();
		System.out.println("Word: "+s);
	    }
	} catch (Exception e) {
	    
	}
    }
}
